@article{delsontro_greenhouse_2018,
  title = {Greenhouse Gas Emissions from Lakes and Impoundments: {{Upscaling}} in the Face of Global Change: {{GHG}} Emissions from Lakes and Impoundments},
  issn = {23782242},
  shorttitle = {Greenhouse Gas Emissions from Lakes and Impoundments},
  doi = {10.1002/lol2.10073},
  abstract = {Lakes and impoundments are important sources of greenhouse gases (GHG: i.e., CO2, CH4, N2O), yet global emission estimates are based on regionally biased averages and elementary upscaling. We assembled the largest global dataset to date on emission rates of all three GHGs and found they covary with lake size and trophic state. Fitted models were upscaled to estimate global emission using global lake size inventories and a remotely sensed global lake productivity distribution. Traditional upscaling approaches overestimated CO2 and N2O emission but underestimated CH4 by half. Our upscaled size-productivity weighted estimates (1.25\textendash{}2.30 Pg of CO2-equivalents annually) are nearly 20\% of global CO2 fossil fuel emission with  75\% of the climate impact due to CH4. Moderate global increases in eutrophication could translate to 5\textendash{}40\% increases in the GHG effects in the atmosphere, adding the equivalent effect of another 13\% of fossil fuel combustion or an effect equal to GHG emissions from current land use change.},
  language = {en},
  journal = {Limnology and Oceanography Letters},
  author = {DelSontro, Tonya and Beaulieu, Jake J. and Downing, John A.},
  month = mar,
  year = {2018},
  file = {/home/jose/Zotero/storage/99KAPILY/DelSontro et al. - 2018 - Greenhouse gas emissions from lakes and impoundmen.pdf}
